{
  "name": "auto-jest-cli",
  "version": "1.0.3",
  "description": "auto-jest",
  "scripts": {
    "build": "tsc",
    "update-cli:local": "npm run build && npm uninstall auto-jest-cli -g && npm install -g . ",
    "preversion": "npm run build && git add dist",
    "patch-release": "npm version patch && git push --follow-tags --force && git reset HEAD~1 --hard",
    "lint": "eslint ./bin --ext .ts",
    "lint:fix": "eslint ./bin --ext .ts --fix",
    "test": "jest --coverage"
  },
  "main": "./dist/index.js",
  "bin": {
    "createJestTemplate": "./dist/initConfig.js"
  },
  "keywords": [
    "auto",
    "jest",
    "cli"
  ],
  "author": "KirillTK",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.0",
    "minimist": "^1.2.6",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/shelljs": "^0.8.11",
    "@types/jest": "^28.1.8",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "7.14.0",
    "jest": "^28.1.3"
  }
}
